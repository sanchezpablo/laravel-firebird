{
    "name": "sanchezpablo/laravel-firebird",
    "description": "A Firebird database package for the Laravel Framework",
    "version": "1.1.0",
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0|~5.8.0",
        "illuminate/container": "~5.5.0|~5.6.0|~5.7.0|~5.8.0",
        "illuminate/database": "~5.5.0|~5.6.0|~5.7.0|~5.8.0",
        "illuminate/events": "~5.5.0|~5.6.0|~5.7.0|~5.8.0",
        "doctrine/instantiator": "~1.1.0"
    },
    "autoload": {
        "psr-4": {
            "Firebird\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Firebird\\FirebirdServiceProvider"
            ]
        }
    }
}
